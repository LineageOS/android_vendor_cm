
BASEDIR=`dirname $0`

mkdir -p $BASEDIR/terminal

# Get Terminal Emulator for Android (we use a prebuilt so it can update from Play Store)
curl -L -o $BASEDIR/terminal/Term.apk -O -L https://jackpal.github.com/Android-Terminal-Emulator/downloads/Term.apk
#unzip -o -d $BASEDIR/proprietaryterminal $BASEDIR/terminal/Term.apk lib/*

